---
export const prerender = false;

import Layout from "../layouts/Layout.astro";
import MainServicios from "../components/MainServicios.astro";
import ServiciosOptions from "../assets/ServiciosOptions.js";

export function getStaticPaths() {
  return ServiciosOptions.map((servicio) => ({
    params: { servicio: servicio.href } // ✅ Ahora es un objeto válido
  }));
}

const { servicio } = Astro.params;
---

<Layout>
  <MainServicios servicio={servicio} />
</Layout>
